                left: 19px;
                font-size: 38px;
            }

            .credit-card-box label {
                font-size: 10px;
                letter-spacing: 1px;
                text-shadow: none;
                text-transform: uppercase;
                font-weight: normal;
                opacity: 0.5;
                display: block;
                margin-bottom: 3px;
            }

            .credit-card-box .card-holder,
            .credit-card-box .card-expiration-date {
                position: absolute;
                margin: 0 auto;
                top: 180px;
                left: 19px;
                font-size: 22px;
                text-transform: capitalize;
            }

            .credit-card-box .card-expiration-date {
                text-align: right;
                left: auto;
                right: 20px;
            }

            .credit-card-box .ccv {
