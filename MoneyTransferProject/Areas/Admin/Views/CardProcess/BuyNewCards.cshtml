<script>
    $(document).ready(function () {
        function setFirstEightDigits(currency) {
            var firstEightDigits;
            switch (currency) {
                case "AZN":
                    firstEightDigits = "1111-1111-@LastEigthNumber";
                    break;
                case "TRL":
                    firstEightDigits = "2222-2222-@LastEigthNumber";
                    break;
                      case "USD":
                    firstEightDigits = "3333-3333-@LastEigthNumber";
                    break;
                case "EUR":
                    firstEightDigits = "4444-4444-@LastEigthNumber";
                    break;
                default:        
                    firstEightDigits = "0000-0000-XXXX-XXXX";
            }
            $("#card-number").val(firstEightDigits);
        }
        setFirstEightDigits($("#senderCardSelect").val());
        $("#senderCardSelect").on("change", function () {
            var selectedCurrency = $(this).val();
            setFirstEightDigits(selectedCurrency);
        });
    });
</script>




  
