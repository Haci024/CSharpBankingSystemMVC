
@{
    Layout = null;
   
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("CustomerHeader")

<body class="nav-md">
    <div class="container body">
        <div class="main_container">

            @await Component.InvokeAsync("CustomerSidebar")

            @await Component.InvokeAsync("CustomerNavbar")

            <div class="" role="main">

                @RenderBody()

            </div>

            @await Component.InvokeAsync("CustomerFooter")

        </div>
    </div>
    @await Component.InvokeAsync("CustomerScript")
</body>
</html>