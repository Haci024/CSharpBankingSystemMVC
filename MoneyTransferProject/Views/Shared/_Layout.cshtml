    <style>
        .logo-container {
            display: inline-flex;
            overflow: hidden;
            position: relative;
            font-size: 72px;
        }

        .pave, .bank {
            opacity: 0;
            animation-duration: 1.2s;
            animation-fill-mode: forwards;
        }

        .pave {
            animation-name: pave-animation;
        }

        .bank {
            animation-name: bank-animation;
        }

        .bank1 {
            margin-left: 8px;
        }

        @@keyframes pave-animation {
            0% {
                transform: translateY(100%);
                opacity: 0;
            }

            100% {
                transform: translateY(0);
                opacity: 1;
            }
        }

        @@keyframes bank-animation {
            0% {
                transform: translateX(100%);
                opacity: 0;
            }

            100% {
                transform: translateX(0);
                opacity: 1;
            }
        }

        .pave:nth-child(1) {
            animation-delay: 0s;
        }

        .pave:nth-child(2) {
            animation-delay: 0.1s;
        }

        .pave:nth-child(3) {
            animation-delay: 0.2s;
        }

        .pave:nth-child(4) {
            animation-delay: 0.3s;
        }

        .pave:nth-child(5) {
            animation-delay: 0.4s;
        }

        .pave:nth-child(6) {
            animation-delay: 0.5s;
        }

        .pave:nth-child(7) {
            animation-delay: 0.6s;
        }


        .bank:nth-child(8) {
            animation-delay: 0.7s;
        }

        .bank:nth-child(9) {
            animation-delay: 0.8s;
        }

        .bank:nth-child(10) {
            animation-delay: 0.9s;
        }

        .bank:nth-child(11) {
            animation-delay: 1.0s;
        }

        .bank:nth-child(12) {
            animation-delay: 1.2s;
        }
    </style>
</head>
